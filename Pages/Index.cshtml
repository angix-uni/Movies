@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<h1 class="text-center">Work in progress</h1>

<ul class="carousel-indicators">
    <li data-target="#slideshow" data-slide-to="0" class="active"></li>
    <li data-target="#slideshow" data-slide-to="1"></li>
    <li data-target="#slideshow" data-slide-to="2"></li>
</ul>

<<div id="slideshow" class="carousel slide" data-ride="carousel" data-interval="3500">
  <div class="carousel-inner">
    <div class="carousel-item active">
            <img class="center" src="~/images/batman.jpg" alt="batman" height="600px" width="450px">
    </div>
    <div class="carousel-item">
            <img class="center" src="~/images/sing.jpg" alt="sing" height="600px" width="450px">
    </div>
    <div class="carousel-item">
            <img class="center" src="~/images/enola.jpg" alt="enola" height="600px" width="450px">
    </div>
  </div>
  <a class="carousel-control-prev" href="#slideshow" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only"/>
  </a>
  <a class="carousel-control-next" href="#slideshow" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only"/>
  </a>
</div>

<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"></script>